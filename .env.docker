# Arquivo de variáveis de ambiente para Docker
# Copie este arquivo para .env e ajuste conforme necessário

# Configurações do Banco de Dados
POSTGRES_DB=reservalab
POSTGRES_USER=reservalab
POSTGRES_PASSWORD=reservalab123

# URL de conexão com o banco (para a aplicação)
DATABASE_URL=postgresql://reservalab:reservalab123@postgres:5432/reservalab?schema=public

# Configurações da aplicação
NODE_ENV=development
PORT=4000

# Configurações do PgAdmin (opcional)
PGADMIN_DEFAULT_EMAIL=admin@reservalab.com
PGADMIN_DEFAULT_PASSWORD=admin123
